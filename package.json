{
  "name": "@shakacode/use-ssr-computation",
  "version": "1.0.3",
  "license": "MIT",
  "scripts": {
    "publish": "lerna publish --ignore-changes '**/*.md' '**/test/*' '**/__snapshots__/*' '**/*.test.{ts,tsx,js,jsx}'",
    "changed": "lerna changed --ignore-changes '**/*.md' '**/test/*' '**/__snapshots__/*' '**/*.test.{ts,tsx,js,jsx}'",
    "version": "lerna version --no-push --ignore-changes '**/*.md' '**/test/*' '**/__snapshots__/*' '**/*.test.{ts,tsx,js,jsx}'",
    "yalc": "cd packages/macro && yalc publish && yalc link && cd ../runtime && yalc publish && yalc link",
    "test": "cd packages/macro && yarn test && cd ../runtime && yarn test"
  },
  "homepage": "https://github.com/Popmenu/use-ssr-computation.macro",
  "bugs": {
    "url": "https://github.com/Popmenu/use-ssr-computation.macro/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Popmenu/use-ssr-computation.macro.git"
  },
  "author": "Roman Kuksin <rkuksin.cpp@gmail.com>",
  "keywords": [
    "SSR",
    "performance",
    "babel",
    "macro",
    "javascript",
    "typescript"
  ],
  "private": true,
  "workspaces": [
    "packages/macro",
    "packages/runtime"
  ],
  "devDependencies": {
    "lerna": "^7.0.2"
  }
}
